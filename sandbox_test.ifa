# If√° Sandbox Test Script

# Import domains
di Odi = gbe("odi");
di Irosu = gbe("irosu");
di Otura = gbe("otura");

Irosu.fo("Starting Sandbox Test...");

# Test 1: File Read (should fail by default if path is outside script dir)
# We will pass a path as argument
di target_file = "C:/Windows/System32/drivers/etc/hosts";
Irosu.so("Attempting to read system file: ");
Irosu.fo(target_file);

tori (Odi.ka(target_file)) {
    Irosu.awo("SUCCESS: Read file!", "red");
} abi {
    Irosu.awo("BLOCKED: Cannot read file", "green");
}

# Test 2: Network (should fail by default)
Irosu.so("Attempting network request... ");
tori (Otura.gba("https://example.com")) {
    Irosu.awo("SUCCESS: Network access!", "red");
} abi {
    Irosu.awo("BLOCKED: Network denied", "green");
}

Irosu.fo("Test Complete.");
